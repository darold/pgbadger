}

sub print_query_per_user
{
	my %infos = ();
	my $total_count = 0;
	my $query_user_info = '';
	my @main_user = ('unknown', 0);
	foreach my $d (sort keys %user_info) {
		$query_user_info .= "<tr><td>$d</td><td>Total</td><td>" .
					&comma_numbers($user_info{$d}{count}) . "</td></tr>";
		$total_count += $user_info{$d}{count};
