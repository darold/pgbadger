			if ((($user_info{$d}{count} * 100) / ($total_count || 1)) > $pie_percentage_limit) {
				$infos{$d} = $user_info{$d}{count} || 0;
			} else {
				$infos{"Sum queries per users < $pie_percentage_limit%"} += $user_info{$d}{count} || 0;
				push(@small, $d);
			}
		}
		if ($#small == 0) {
